&axi_ethernet_0 {
    local-mac-address = [00 0a 35 00 01 22];
    phy-handle = <&phy0>;
    xlnx,has-mdio = <0x1>;
    phy-mode = "gmii";
    mdio {
        #address-cells = <1>;
        #size-cells = <0>;
        phy0: phy@1 {
            device_type = "ethernet-phy";
            xlnx,phy-type = <0x5>;
            reg = <1>;
        };
    };
};

